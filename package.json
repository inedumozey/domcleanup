{
  "name": "@mozeyinedu/domcleanup",
  "version": "0.2.2",
  "description": "it purifies the dom against XSS attacks. It is built on top of JSDOM and dompurify",
  "main": "lib/esm/index.js",
  "module": "lib/cjs/index.js",
  "scripts": {
    "build": "rm -rf lib/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJs --outDir lib/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/inedumozey/domcleanup.git"
  },
  "keywords": [
    "dom",
    "clean",
    "typescript",
    "up",
    "dompurify",
    "XSS",
    "attacks"
  ],
  "author": "drmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inedumozey/domcleanup/issues"
  },
  "homepage": "https://github.com/inedumozey/domcleanup#readme",
  "dependencies": {
    "dompurify": "^3.0.5",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.3",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.2",
    "typescript": "^5.2.2"
  }
}
